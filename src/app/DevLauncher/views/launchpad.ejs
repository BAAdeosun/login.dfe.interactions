    <div class="row">
        <div class="col-12">
            <p>
                UUID is <%= uuid %>
            </p>
        </div>
    </div>
    <div class="cards">
        <a class="card" href="/<%= uuid %>/usernamepassword">
            <h2>Start username/password</h2>
            <p>Start the default username and password login journey</p>
        </a>
        <a class="card" href="/dev/complete">
            <h2>Interaction Complete</h2>
            <p>Show the Interaction Complete page</p>
        </a>
        <a class="card" href="/<%= uuid %>/resetpassword/request">
            <h2>Start reset password</h2>
            <p>Start the reset password journey</p>
        </a>
        <a class="card" href="/<%= uuid %>/resetpassword/confirm">
            <h2>Confirm reset password</h2>
            <p>Enter code and continue with password reset</p>
        </a>
    </div>
